//array of objects//
#include<iostream>
using namespace std;
class identity{
    public:
    int getdata(){
        cout<<"my name is anushka"<<endl;
    }
    int putdata(){
        cout<<"i love to draw something great things"<<endl;
    }
};
int main(){
    identity id[4];
    for(int i=0;i<=4;i++){
        id[i].getdata();
        id[i].putdata();
    }
    return 0;
}
