/*
using vector
*/ 
#include <iostream>
#include<vector>
using namespace std;
 int linearsearch( vector<int> &num, int &target){
     for(int i =0;i<num.size();i++){
         if(num.at(i)==target){
             return i;
         }
 }
     return -1;
 }
int main() {
    // Write C++ code here
    vector<int>vec={2,4,6,7,3,9};
    int no=9;
    cout<<linearsearch(vec,no);

    return 0;
}
