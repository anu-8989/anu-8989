
//linear-search//
//TC=(O(n))//
#include <iostream>
using namespace std;
int linearsearch(int arr[],int sz,int target){
    for(int i=0;i<sz;i++){
    if(arr[i]==target){
        return 1;
    }
    }
    return -1;
}
int main() {
    int size=6;
    int arr[]={1,2,3,4,5,6};
    int target=0;
    cout<<linearsearch(arr,size,target);
    return 0;
}
